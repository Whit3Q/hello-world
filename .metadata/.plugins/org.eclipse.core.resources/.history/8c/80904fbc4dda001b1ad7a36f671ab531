
public class Ex2_1 {

	public static void main(String[] args) {
		
		int code = 65;
		char ch = char(code);
		
		System.out.println(code);
		System.out.println(ch);
		
		
		/*
		 * CharToCode.java 2-1
		 * char ch = 'A'; int code = (int)ch;
		 * 
		 * System.out.print(ch); System.out.println(code);
		 * 실행결과 
		 * A65
		 */	
		
	}

}