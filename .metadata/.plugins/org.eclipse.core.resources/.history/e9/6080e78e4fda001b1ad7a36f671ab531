
public class Ex2_1 {

	public static void main(String[] args) {
		
		byte b = 1;
		short s = 2;
		int finger = 10;
		long big = 100000000000;
		
		System.out.println(b);
		System.out.println(s);
		System.out.println(finger);
		System.out.println(big);
		
		
		/*
		 * char single ='\''; String dblQuote = "\"Hello\""; String root = "c:\\";
		 * String name = "Ja"+"va";
		 * 
		 * System.out.println(single); System.out.println(dblQuote);
		 * System.out.println(root); System.out.println(name);
		 */
		/*
		 *  CharToCode.java 2-1
		 *
		 * int code = 65; char ch = (char)code;
		 * 
		 * System.out.println(code); System.out.println(ch);
		 * 
		 * 실행결과
		 * 65
		 * A		
		 */
		
		
		/*
		 * CharToCode.java 2-1
		 * char ch = 'A'; int code = (int)ch;
		 * 
		 * System.out.print(ch); System.out.println(code);
		 * 실행결과 
		 * A65
		 */	
		
	}

}