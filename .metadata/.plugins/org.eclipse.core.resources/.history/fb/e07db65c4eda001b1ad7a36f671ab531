
public class Ex2_1 {

	public static void main(String[] args) {
		
		char single ='\'';
		String dblQuote = "\"Hello\"";
		String root = "c:\\";
		System.out.println(single);
		System.out.println(dblQuote);
		System.out.println(root);
		
		/*
		 *  CharToCode.java 2-1
		 *
		 * int code = 65; char ch = (char)code;
		 * 
		 * System.out.println(code); System.out.println(ch);
		 * 
		 * 실행결과
		 * 65
		 * A		
		 */
		
		
		/*
		 * CharToCode.java 2-1
		 * char ch = 'A'; int code = (int)ch;
		 * 
		 * System.out.print(ch); System.out.println(code);
		 * 실행결과 
		 * A65
		 */	
		
	}

}